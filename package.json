{
  "name": "AssigmentProject",
  "description": "NodeJs Express with Typescript",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "start:dev": "nodemon",
    "start": "npm run build && node dist/server.js",
    "dev-server": "tsc-watch --noClear -p tsconfig.json --onSuccess \"node ./dist/server.js\"",
    "start:prod": "node dist/app/server.js",
    "lint": "tslint --project tsconfig.json",
    "test": "jest",
    "watch": "nodemon -e ts -w ./src -x npm run watch:serve",
    "watch:serve": "ts-node --inspect src/server.ts"
  },
  "author": "Himanshu Malvi <himanshumalsviya2014@gmail.com>",
  "dependencies": {
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.178",
    "@types/method-override": "0.0.31",
    "@types/mongodb": "^3.1.22",
    "@types/mongoose": "^5.3.21",
    "@types/node": "^11.11.1",
    "@types/request-promise": "^4.1.48",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "inversify": "^4.13.0",
    "inversify-express-utils": "^6.1.0",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.19",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.2",
    "rxjs-compat": "^6.6.7",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.6.0",
    "tslint": "^6.1.3"
  }
}
